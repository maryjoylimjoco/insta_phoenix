<h2>Show post</h2>

<ul>

  <li>
    <strong>User:</strong>
    <%= @post.user_id %>
  </li>

  <li>
    <strong>Photo:</strong>
    <%= @post.photo %>
  </li>

  <li>
    <strong>Caption:</strong>
    <%= @post.caption %>
  </li>

</ul>

<%= link "Edit", to: post_path(@conn, :edit, @post) %>
<%= link "Back", to: post_path(@conn, :index) %>
